{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        { "src": "/", "dest": "/public/index.html" },
        { "src": "/login", "dest": "/public/login.html" },
        { "src": "/signup", "dest": "/public/signup.html" },
        { "src": "/home", "dest": "/public/home.html" },
        { "src": "/(.*)", "dest": "/index.js"}
    ]
  }
  